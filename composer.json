{
  "name": "ssitdikov/atol",
  "description": "ATOL API Client. Данная библиотека предназначена для работы с сервисом АТОЛ Онлайн",
  "keywords": ["atol", "api", "online", "php"],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Salavat Sitdikov",
      "email": "sitsalavat@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "SSitdikov\\ATOL\\": "src/",
      "SSitdikov\\ATOL\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "php-coveralls/php-coveralls": "^1.0",
    "phpunit/phpcov": "^4.0",
    "codacy/coverage": "^1.4"
  }
}
